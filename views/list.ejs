<%- include("header") -%>

   <div class="box" id="heading">
    <h1>It's a <%= header%>!</h1>
   </div>
  <div class="box">
    <%for(var i=0;i<data.length;i++){%>
      <div class="item">
        <input type="checkbox">
        <p><%= data[i]%></p>
      </div>
        <%}%>
        <form class="item" action="/" method="post">
          <input type="text" name="itemAdded" placeholder="New item" autocomplete="off">
          <button type="submit" name="button" value=<%= header%>>+</button>
        </form>
</div>
<%- include("footer") -%>
